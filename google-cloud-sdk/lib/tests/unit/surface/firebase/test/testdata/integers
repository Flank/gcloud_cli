# Valid int args

scenario1:
  scenario-numbers: 2000

scenario4:
  scenario-numbers:
    - 1
    - 3
    - 13
    - 77

# Invalid int lists

scenario-zero:
  scenario-numbers: 0  # scenario #s start at 1

scenario-neg:
  scenario-numbers: -1024  # scenario #s start at 1

scenario-neg-in-list:
  scenario-numbers: [3, -1, 5]

scenario-float:
  scenario-numbers: [1, 3.14]

scenario-str:
  scenario-numbers: 1a

scenario-str-list:
  scenario-numbers: ['1', '3', '5', '7']

scenario-dict:
  scenario-numbers:
    foo: 2
    bar: 4

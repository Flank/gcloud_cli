{
  "--file-share":
    {
      "capacity": "1024",
      "name": "my_vol",
      "nfs-export-options": [{
        "access-mode": "READ_WRITE",
        "ip-ranges": [
          "10.11.0.0/24",
          "10.12.0.0/24"
        ],
        "squash-mode": "ROOT_SQUASH"
      },
        {
          "access-mode": "READ_ONLY",
          "ip-ranges": [
            "10.13.0.0/24"
          ],
          "squash-mode": "NO_ROOT_SQUASH"
        }
      ]
    }

}

